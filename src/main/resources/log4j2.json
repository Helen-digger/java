{
  "configuration": {
    "name": "Default",
    "appenders": {
      "RollingFile": {
        "name":"File",
        "fileName":"logs/log.html",
        "filePattern":"debag-backup-%d{MM-dd-yy-HH-mm-ss}-%i.html.gz",
        "HTMLLayout": {
          "charset":"UTF-8",
          "title":"Dashboard s logs",
          "locationInfo":"true"
        },
        "Policies": {
          "SizeBasedTriggeringPolicy": {
            "size":"10 MB"
          }
        },
        "DefaultRolloverStrategy": {
          "max":"10"
        }
      }
    },
    "loggers": {
      "root": {
        "level":"debug",
        "appender-ref": {
          "ref":"File"
        }
      }
    }
  }
}
